#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/bryan.robitaille/repos/zitadel/node_modules/.pnpm/vite@5.4.20_@types+node@24.3.1_less@4.1.3_lightningcss@1.30.1_sass@1.92.1_terser@5.44.0/node_modules/vite/bin/node_modules:/Users/bryan.robitaille/repos/zitadel/node_modules/.pnpm/vite@5.4.20_@types+node@24.3.1_less@4.1.3_lightningcss@1.30.1_sass@1.92.1_terser@5.44.0/node_modules/vite/node_modules:/Users/bryan.robitaille/repos/zitadel/node_modules/.pnpm/vite@5.4.20_@types+node@24.3.1_less@4.1.3_lightningcss@1.30.1_sass@1.92.1_terser@5.44.0/node_modules:/Users/bryan.robitaille/repos/zitadel/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/bryan.robitaille/repos/zitadel/node_modules/.pnpm/vite@5.4.20_@types+node@24.3.1_less@4.1.3_lightningcss@1.30.1_sass@1.92.1_terser@5.44.0/node_modules/vite/bin/node_modules:/Users/bryan.robitaille/repos/zitadel/node_modules/.pnpm/vite@5.4.20_@types+node@24.3.1_less@4.1.3_lightningcss@1.30.1_sass@1.92.1_terser@5.44.0/node_modules/vite/node_modules:/Users/bryan.robitaille/repos/zitadel/node_modules/.pnpm/vite@5.4.20_@types+node@24.3.1_less@4.1.3_lightningcss@1.30.1_sass@1.92.1_terser@5.44.0/node_modules:/Users/bryan.robitaille/repos/zitadel/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../vite/bin/vite.js" "$@"
fi
